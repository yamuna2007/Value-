#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    int sum = 0;

    // Read n integers and calculate sum
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    // Calculate mean
    float mean = (float)sum / n;

    // Print with 2 decimal places
    printf("Array Mean Value is %.2f\n", mean);

    return 0;
}
