#include <stdio.h>

int main() {
    int n, num, sum = 0;
    scanf("%d", &n);  // Read size of array

    for (int i = 0; i < n; i++) {
        scanf("%d", &num);  // Read each number

        if (num > 0) {
            sum += num * num;  // Add square to sum if positive
        }
    }

    printf("%d\n", sum);  // Print the result
    return 0;
}
