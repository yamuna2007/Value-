#include <stdio.h>

int fibonacci(int n) {
    if (n == 1)
        return 0;
    else if (n == 2)
        return 1;
    else {
        int a = 0, b = 1, c;
        for (int i = 3; i <= n; i++) {
            c = a + b;
            a = b;
            b = c;
        }
        return b;
    }
}

int main() {
    int n, m, sum = 0;

    // Read input
    if (scanf("%d%d", &n, &m) != 2) {
        printf("Invalid Input\n");
        return 0;
    }

    // Constraint check
    if (n < 1 || n > 20 || m < 1 || m > 20) {
        printf("Invalid Input\n");
        return 0;
    }

    // Ensure n is less than or equal to m
    if (n > m) {
        int temp = n;
        n = m;
        m = temp;
    }

    // Sum the Fibonacci numbers from n to m
    for (int i = n; i <= m; i++) {
        sum += fibonacci(i);
    }

    // Output with .0 format as shown in problem
    printf("The Sum of Fibonacci value is %d.0\n", sum);

    return 0;
}
